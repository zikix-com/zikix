{
  "name": "zikix/laravel-component",
  "description": "The Zikix Laravel Components package.",
  "keywords": [
    "support"
  ],
  "license": "MIT",
  "homepage": "https://zikix.com",
  "support": {
    "issues": "https://github.com/zikix-com/support/issues",
    "source": "https://github.com/zikix-com/support"
  },
  "authors": [
    {
      "name": "Zikix",
      "email": "i@zikix.com"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.0-dev"
    },
    "laravel": {
      "providers": [
        "Lokielse\\LaravelSLS\\LaravelSLSServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=5.5",
    "ext-xml": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-curl": "*",
    "ext-zip": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35|^5.4.3",
    "laravel/framework": "^5.8"
  },
  "autoload": {
    "psr-4": {
      "Zikix\\LaravelComponent\\": "src/"
    },
    "files": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Zikix\\LaravelComponent\\Test\\": "tests/"
    }
  },
  "scripts": {
    "fixer": "php-cs-fixer fix ./"
  }
}
